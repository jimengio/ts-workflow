{
  "name": "mst-workflow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "serve": "http-server dist/ -s",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "upload": "rsync -avr --progress dist/ fe.jimu.io:~/repo/jimengio/ts-workflow",
    "gen-router": "webpack --config webpack/router-config.js && node dist/gen-router.js",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jimengio/ci-progress-webpack-plugin": "^0.1.0",
    "@jimengio/router-code-generator": "^0.1.5",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/prettier": "^1.16.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "cache-loader": "^4.0.0",
    "css-loader": "^3.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.12",
    "http-server": "^0.11.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.1",
    "typescript-styled-plugin": "^0.14.0",
    "url-loader": "^2.0.0",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {
    "@jimengio/ruled-router": "^0.2.12",
    "dayjs": "^1.8.13",
    "emotion": "^10.0.9",
    "font-awesome": "^4.7.0",
    "immer": "^3.1.3",
    "lodash": "^4.17.11",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
